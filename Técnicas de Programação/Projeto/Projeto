iple clicks.\n","      quickchartButtonEl.classList.add('colab-df-spinner');\n","      try {\n","        const charts = await google.colab.kernel.invokeFunction(\n","            'suggestCharts', [key], {});\n","      } catch (error) {\n","        console.error('Error during call to suggestCharts:', error);\n","      }\n","      quickchartButtonEl.classList.remove('colab-df-spinner');\n","      quickchartButtonEl.classList.add('colab-df-quickchart-complete');\n","    }\n","    (() => {\n","      let quickchartButtonEl =\n","        document.querySelector('#df-e7493220-f032-4110-ae44-a255b46ec664 button');\n","      quickchartButtonEl.style.display =\n","        google.colab.kernel.accessAllowed ? 'block' : 'none';\n","    })();\n","  </script>\n","</div>\n","    </div>\n","  </div>\n"]},"metadata":{},"execution_count":12}]},{"cell_type":"code","source":["# Combine os DataFrames com base na coluna \"BAIRRO\" do DataFrame das escolas e \"nome\" do DataFrame das subprefeituras\n","df_completo = df_escolas.merge(df_subprefeituras, left_on='bairro', right_on='nome', how='left')\n","\n","# Remova a coluna \"nome\" duplicada, se desejado\n","df_completo.drop('nome', axis=1, inplace=True)\n","df_completo"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":493},"id":"UMXN0mL10YnZ","executionInfo":{"status":"ok","timestamp":1694609716185,"user_tz":180,"elapsed":22,"user":{"displayName":"Mariana Ogawa","userId":"12841206806853258763"}},"outputId":"86637007-fb42-4cdb-da3f-1d0f095105d0"},"execution_count":13,"outputs":[{"output_type":"execute_result","data":{"text/plain":["      id             bairro       lat       lon tipo_escola  \\\n","0    178               CAJU -22.88089 -43.22533         CIE   \n","1    634            BENFICA -22.88957 -43.23620          EM   \n","2    483             CENTRO -22.90929 -43.18358          EM   \n","3    476      SAO CRISTOVAO -22.89763 -43.22746          EM   \n","4    132       RIO COMPRIDO -22.92441 -43.20858          EM  